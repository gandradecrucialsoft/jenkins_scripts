//http://tdongsi.github.io/blog/2017/04/18/groovy-code-in-jenkins-pipeline/

def code

node{
    stage("Checkout"){
        git credentialsId: 'git_cred', url: 'https://github.com/gandradecrucialsoft/jenkins_scripts.git'
    }
    stage("Load"){
        code = load 'example.groovy'
    }
    stage('Execute') {
        code.example1()
    }
}
code.example2()
